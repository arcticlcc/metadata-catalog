{
  "name": "fabpot/silex-skeleton",
  "description": "A pre-configured skeleton for the Silex microframework",
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=5.5.9",
    "silex/silex": "~2.0@dev",
    "silex/web-profiler": "~2.0@dev",
    "symfony/browser-kit": "~2.6",
    "symfony/class-loader": "~2.6",
    "symfony/config": "~2.6",
    "symfony/console": "~2.6",
    "symfony/css-selector": "~2.6",
    "symfony/debug": "~2.6",
    "symfony/finder": "~2.6",
    "symfony/form": "~2.6",
    "symfony/monolog-bridge": "~2.6",
    "symfony/process": "~2.6",
    "symfony/security": "~2.6",
    "symfony/translation": "~2.6",
    "symfony/twig-bridge": "~2.6",
    "symfony/validator": "~2.6",
    "dflydev/doctrine-orm-service-provider": "^2.0",
    "deralex/yaml-config-service-provider": "2.0.x-dev",
    "saxulum/saxulum-doctrine-orm-commands": "~1.0.0",
    "saxulum/saxulum-doctrine-orm-manager-registry-provider": "~3.0",
    "boldtrn/jsonb-bundle": "^1.0",
    "opensoft/doctrine-postgres-types": "^1.1",
    "saxulum/saxulum-bootstrap-provider": "dev-master",
    "twbs/bootstrap": "~3.3",
    "saxulum/saxulum-assetic-twig-provider": "~2.0",
    "natxet/cssmin": "~3.0.1",
    "mrclay/minify": "~2.2.0",
    "oyejorge/less.php": "~1.7",
    "saxulum/saxulum-console": "~3.0"
  },
  "autoload": {
    "psr-0": {
      "MetaCat": "src/",
      "": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  },
  "scripts": {
    "run": ["echo 'Started web server on http://localhost:8888'", "php -S localhost:8888 -t web web/index_dev.php"]
  }
}
